#include <iostream>
#include <vector>
#include <queue>
using namespace std;

bool dfs(const vector<vector<int>> &adj, int node, vector<bool> &visited,int parent) {
    visited[node]=true;
    for(int x:adj[node]){
        if(visited[x]&&x!=parent){
            return true;
            
        }
        else{
            if(dfs(adj,x,visited,node)){
                return true;
            }
        }
    }
    return false;
}